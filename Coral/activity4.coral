integer userAgeYears
integer userAgeDays
integer userAgeMinutes

userAgeYears = Get next input
userAgeDays = userAgeYears * 365
userAgeDays = userAgeDays + (userAgeYears / 4)

Put "You are " to output
Put userAgeDays to output
Put " days old.\n" to output

userAgeMinutes = userAgeDays * 24 * 60
Put "You are " to output
Put userAgeMinutes to output
Put " minutes old. \n" to output
